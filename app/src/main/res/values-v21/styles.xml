        <item name="colorPrimary">@color/myblack</item>
        <item name="colorPrimaryDark">?android:attr/colorBackground</item>
        <item name="colorAccent">@color/myred</item>
        <item name="android:colorPrimary">@color/bloodreddark</item>
        <item name="android:colorPrimaryDark">@android:color/secondary_text_light</item>
        <item name="android:statusBarColor">@color/bloodredlight</item>
        <item name="android:navigationBarColor">@color/bloodredlight</item>
    </style>
</resources>